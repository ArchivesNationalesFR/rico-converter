<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:html="http://www.w3.org/1999/xhtml" xmlns:isni="http://isni.org/ontology#"
   xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:rico="http://www.ica.org/standards/RiC/ontology#"
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xml:base="http://data.archives-nationales.culture.gouv.fr/">
   <rico:FindingAid rdf:about="findingAid/011222">
      <rico:hasMainSubject rdf:resource="recordResource/011222-top"/>
   
      <rico:hasInstantiation rdf:resource="instantiation/011222-i1"/>
   </rico:FindingAid>
   <rico:Instantiation rdf:about="instantiation/011222-i1">
      <rico:instantiates rdf:resource="findingAid/011222"/>
      <rico:encodingFormat xml:lang="en">text/xml</rico:encodingFormat>
      <dc:format xml:lang="en">text/xml</dc:format>
      <rico:identifier>FRAN_IR_011222</rico:identifier>
      <rico:heldBy rdf:resource="agent/005061"/>
      <rdfs:seeAlso rdf:resource="https://www.siv.archives-nationales.culture.gouv.fr/siv/IR/FRAN_IR_011222"/>     
   </rico:Instantiation>
   <!-- The highest level Record resource, corresponding to archdesc, and its physical or digital primary instantiation
            If there is an @id, use {findingAidId}{@id}; if no @id for archdesc, use {findingAidId}-top -->
   <rico:RecordResource rdf:about="recordResource/011222-top">
      <rico:isMainSubjectOf rdf:resource="findingAid/011222"/>
      <rico:hasInstantiation rdf:resource="instantiation/011222-top-i1"/>
      <rico:hasMember
         rdf:resource="recordResource/011222-foo"/>
      <rico:hasMember
         rdf:resource="recordResource/011222-klmn"/>
   </rico:RecordResource>
   <rico:Instantiation rdf:about="instantiation/011222-top-i1">
      <rico:instantiates rdf:resource="recordResource/011222-top"/>
      <rdfs:seeAlso rdf:resource="https://www.siv.archives-nationales.culture.gouv.fr/siv/UD/FRAN_IR_011222/top"/>
   </rico:Instantiation>
   <!-- then there should be another RDF file for outputting the RDF data about the c elements (one file per c, child of archdesc)-->
   <!-- so from the input.xml file, we should get 2 more files
      In the first file we should find the following, which corresponds to the 'foo' c element and its subcomponents -->
   
  <rico:RecordResource rdf:about="recordResource/011222-foo">
      <rico:isMemberOf rdf:resource="recordResource/011222-top"/>
      <rico:hasInstantiation rdf:resource="instantiation/011222-foo-i1"/>
      <rico:hasMember
         rdf:resource="recordResource/011222-abcdefg"/>
   </rico:RecordResource>
   <rico:Instantiation rdf:about="instantiation/011222-foo-i1">
      <rico:instantiates rdf:resource="recordResource/011222-foo"/>
      <rdfs:seeAlso rdf:resource="https://www.siv.archives-nationales.culture.gouv.fr/siv/UD/FRAN_IR_011222/foo"/>
   </rico:Instantiation>
   <rico:RecordResource rdf:about="recordResource/011222-abcdefg">
      <rico:isMemberOf rdf:resource="recordResource/011222-foo"/>
      <rico:hasInstantiation rdf:resource="instantiation/011222-abcdefg-i1"/>
   </rico:RecordResource>
   <rico:Instantiation rdf:about="instantiation/011222-abcdefg-i1">
      <rico:instantiates rdf:resource="recordResource/011222-abcdefg"/>
      <rdfs:seeAlso rdf:resource="https://www.siv.archives-nationales.culture.gouv.fr/siv/UD/FRAN_IR_011222/abcdefg"/>
   </rico:Instantiation>
   
  
   <!-- in the 2nd file we should find the following-->
   <rico:RecordResource rdf:about="recordResource/011222-klmn">
      <rico:isMemberOf rdf:resource="recordResource/011222-top"/>
      <rico:hasInstantiation rdf:resource="instantiation/011222-klmn-i1"/>
   
   </rico:RecordResource>
   <rico:Instantiation rdf:about="instantiation/011222-klmn-i1">
      <rico:instantiates rdf:resource="recordResource/011222-klmn"/>
      <rdfs:seeAlso rdf:resource="https://www.siv.archives-nationales.culture.gouv.fr/siv/UD/FRAN_IR_011222/klmn"/>
   </rico:Instantiation>
</rdf:RDF>
