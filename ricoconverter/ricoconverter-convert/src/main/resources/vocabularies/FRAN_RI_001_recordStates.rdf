   <skos:Concept rdf:about="recordState/FRAN_RI_001-d-9ag71ol70-l5bgs9hqcofr">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordState"/>
      <skos:inScheme rdf:resource="recordStates"/>
      <skos:prefLabel xml:lang="fr">copie sous le sceau</skos:prefLabel>
      <skos:definition xml:lang="fr">« Une copie sous le sceau est une copie dont l’authenticité est attestée par l'apposition du sceau d'une autorité. En Angleterre, cette pratique est dite exemplification. » (source : Vocabulaire international de diplomatique, 2e éd., 1997, p. 33.) </skos:definition>
      <skos:changeNote>
         <rdf:Description>
            <dc:date>2024</dc:date>
            <rdf:value xml:lang="fr">préparé par J.-F. Moufflet [2024] ; ajouté par le Lab le [19/07/2024]</rdf:value>
         </rdf:Description>
      </skos:changeNote>
      <skos:exactMatch rdf:resource="http://www.cei.lmu.de/VID/#VID_61"/>
      <skos:broader rdf:resource="recordState/FRAN_RI_001-d3m00w6tom1--17yz38y8bszt8"/>
