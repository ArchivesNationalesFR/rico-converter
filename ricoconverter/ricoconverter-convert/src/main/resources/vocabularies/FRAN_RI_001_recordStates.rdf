   <skos:Concept rdf:about="recordState/FRAN_RI_001-d-97at5bjsw--u688kxdppcy9">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordState"/>
      <skos:inScheme rdf:resource="recordStates"/>
      <skos:prefLabel xml:lang="fr">copie de chancellerie</skos:prefLabel>
      <skos:definition xml:lang="fr">« Une copie de chancellerie est la copie d'un original, établie par la chancellerie même qui a procédé à l'expédition de cet original. » (source : Vocabulaire international de diplomatique, 2e éd., 1997, p. 33.) </skos:definition>
      <skos:changeNote>
         <rdf:Description>
            <dc:date>2024</dc:date>
