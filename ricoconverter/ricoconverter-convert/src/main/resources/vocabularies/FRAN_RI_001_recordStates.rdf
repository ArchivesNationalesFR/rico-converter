   <skos:Concept rdf:about="recordState/FRAN_RI_001-d-985uqpl3s-1k3i4yfl1myau">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordState"/>
      <skos:inScheme rdf:resource="recordStates"/>
      <skos:prefLabel xml:lang="fr">copie enregistrée</skos:prefLabel>
      <skos:definition xml:lang="fr">« Une copie enregistrée (lat. : copia registrata, litterae registratae, registrum) est une transcription faite par un service qui a autorité pour enregistrer les actes et leur donner ainsi un caractère d'authenticité. » (source : Vocabulaire international de diplomatique, 2e éd., 1997, p. 33.) </skos:definition>
      <skos:changeNote>
         <rdf:Description>
            <dc:date>2024</dc:date>
            <rdf:value xml:lang="fr">préparé par J.-F. Moufflet [2024] ; ajouté par le Lab le [19/07/2024]</rdf:value>
         </rdf:Description>
      </skos:changeNote>
      <skos:exactMatch rdf:resource="http://www.cei.lmu.de/VID/#VID_58"/>
      <skos:broader rdf:resource="recordState/FRAN_RI_001-d3m00w6tom1--17yz38y8bszt8"/>
   </skos:Concept>
   <skos:Concept rdf:about="recordState/FRAN_RI_001-d-98yheu10c--1dhxakzzoeqts">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordState"/>
      <skos:inScheme rdf:resource="recordStates"/>
      <skos:prefLabel xml:lang="fr">copie judiciaire</skos:prefLabel>
      <skos:definition xml:lang="fr">« Une copie judiciaire est une copie dressée sur l'ordre d'un juge et (ou) pourvue d'un certificat d'authenticité de celui-ci. » (source : Vocabulaire international de diplomatique, 2e éd., 1997, p. 33.) </skos:definition>
      <skos:changeNote>
         <rdf:Description>
            <dc:date>2024</dc:date>
            <rdf:value xml:lang="fr">préparé par J.-F. Moufflet [2024] ; ajouté par le Lab le [19/07/2024]</rdf:value>
         </rdf:Description>
      </skos:changeNote>
      <skos:exactMatch rdf:resource="http://www.cei.lmu.de/VID/#VID_60"/>
      <skos:broader rdf:resource="recordState/FRAN_RI_001-d3m00w6tom1--17yz38y8bszt8"/>
   </skos:Concept>
   <skos:Concept rdf:about="recordState/FRAN_RI_001-d-99t1j5n3c--1obbuv2ji4yy2">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordState"/>
      <skos:inScheme rdf:resource="recordStates"/>
      <skos:prefLabel xml:lang="fr">copie notariée</skos:prefLabel>
      <skos:definition xml:lang="fr">« Une copie notariée est une copie qui comporte un certificat d'authenticité de la main d'un ou de plusieurs notaires. » (source : Vocabulaire international de diplomatique, 2e éd., 1997, p. 33.) </skos:definition>
      <skos:changeNote>
         <rdf:Description>
            <dc:date>2024</dc:date>
            <rdf:value xml:lang="fr">préparé par J.-F. Moufflet [2024] ; ajouté par le Lab le [19/07/2024]</rdf:value>
         </rdf:Description>
