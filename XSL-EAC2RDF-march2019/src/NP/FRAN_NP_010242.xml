         <functions>
            <function>
               <term vocabularySource="d3nyui3o8w--11y7jgy8q3wnt">notaire à paris</term>
               <dateRange>
                  <fromDate standardDate="1591-01-01">1591</fromDate>
                  <toDate/>
               </dateRange>
            </function>
