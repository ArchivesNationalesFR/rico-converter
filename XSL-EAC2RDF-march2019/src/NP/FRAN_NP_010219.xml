               <placeRole>Lieu de Paris</placeRole>
               <placeEntry localType="nomLieu">10 rue Royale, 75008 Paris</placeEntry>
               <placeEntry localType="voie" vocabularySource="d3nzczqqga-1kszjfepo01fw">Royale (rue)</placeEntry>
