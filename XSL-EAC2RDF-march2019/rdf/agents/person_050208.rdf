      <rdfs:label xml:lang="fr">Walter, Jean (1883-1957)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050208_d3822e47"/>
