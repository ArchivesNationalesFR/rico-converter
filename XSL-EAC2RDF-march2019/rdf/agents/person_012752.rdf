      <rdfs:label xml:lang="fr">Bailly, Mathieu</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012752_d2851e45"/>
