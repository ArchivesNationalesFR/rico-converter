      <rdfs:label xml:lang="fr">Marrou, Jean (1933-....)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/009254_d179e65"/>
