      <rdfs:label xml:lang="fr">Lambon, Nicolas de</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011330_d1429e41"/>
