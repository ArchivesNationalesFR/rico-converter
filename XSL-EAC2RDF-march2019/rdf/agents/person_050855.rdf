      <rdfs:label xml:lang="fr">Le Guillant, Louis (1900-1968)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050855_d4282e45"/>
