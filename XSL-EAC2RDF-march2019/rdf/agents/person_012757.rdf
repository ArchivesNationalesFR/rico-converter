      <rdfs:label xml:lang="fr">Ballot, Sylvain</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012757_d2856e55"/>
