      <rdfs:label xml:lang="fr">Vaublanc, Charles Viénot de (1721-1804)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051674_d4868e57"/>
