      <rdfs:label xml:lang="fr">Chenot, Bernard (1909-1995)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051653_d4849e55"/>
