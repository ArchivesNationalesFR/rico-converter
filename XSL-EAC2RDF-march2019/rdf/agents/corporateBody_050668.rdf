      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">Commandement militaire du Palais de l'Elysée</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050668_d4160e34"/>
