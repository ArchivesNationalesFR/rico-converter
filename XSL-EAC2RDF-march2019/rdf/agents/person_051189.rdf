   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/person/051189">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
      <rdfs:label xml:lang="fr">Despoisse, Claude (1922-2014)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051189_d4489e53"/>
