      <rdfs:label xml:lang="fr">Mun, Albert de</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050139_d3764e34"/>
