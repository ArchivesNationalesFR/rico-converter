   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/corporateBody/003773">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">École des hautes études en sciences sociales (Paris ; 1975-....)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/003773_d42e84"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/003773_d42e87"/>
