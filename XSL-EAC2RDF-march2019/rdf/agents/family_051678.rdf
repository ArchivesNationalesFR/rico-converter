      <rdfs:label xml:lang="fr">Ratton (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051678_d4871e52"/>
