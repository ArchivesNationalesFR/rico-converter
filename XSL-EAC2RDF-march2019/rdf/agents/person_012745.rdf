      <rdfs:label xml:lang="fr">Badenier, Edme</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012745_d2844e41"/>
