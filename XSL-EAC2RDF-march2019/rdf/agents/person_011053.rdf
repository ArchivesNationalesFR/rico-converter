      <rdfs:label xml:lang="fr">Maigret, Bernard</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011053_d1152e47"/>
