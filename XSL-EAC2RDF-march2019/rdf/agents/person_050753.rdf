      <RiC:deathDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1962-08-01</RiC:deathDate>
      <RiC:personIsSourceOfOccupationRelation rdf:resource="http://data.archives-nationales.culture.gouv.fr/occupationRelation/050753-d4220e180"/>
