      <rdfs:label xml:lang="fr">Coty, René</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050284_d3874e43"/>
