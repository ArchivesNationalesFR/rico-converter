      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
      <rdfs:label xml:lang="fr">Rouen, Denis-André</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/010728_d827e41"/>
