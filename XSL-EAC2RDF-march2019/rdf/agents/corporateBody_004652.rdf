   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/corporateBody/004652">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">Caisse nationale des allocations familiales (France)</rdfs:label>
