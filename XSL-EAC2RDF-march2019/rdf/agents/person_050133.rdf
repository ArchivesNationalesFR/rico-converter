      <rdfs:label xml:lang="fr">Laurentie, François (1874-1915)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050133_d3760e43"/>
