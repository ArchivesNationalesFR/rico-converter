      <rdfs:label xml:lang="fr">Ormesson, Wladimir d' (1888-1973)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050563_d4093e43"/>
