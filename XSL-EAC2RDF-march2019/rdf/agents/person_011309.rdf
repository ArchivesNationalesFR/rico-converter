      <rdfs:label xml:lang="fr">Laideguive, Antoine-Pierre</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011309_d1408e41"/>
