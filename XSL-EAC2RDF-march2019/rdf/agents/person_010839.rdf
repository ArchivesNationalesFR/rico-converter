      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
      <rdfs:label xml:lang="fr">Parque, Pierre I (15..-16..)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/010839_d938e64"/>
