      <rdfs:label xml:lang="fr">France. Commission interministérielle des régies municipales</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050838_d4270e36"/>
