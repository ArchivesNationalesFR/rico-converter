      <rdfs:label xml:lang="fr">Paulze d'Ivoy, Jacques Christian</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050535_d4065e39"/>
