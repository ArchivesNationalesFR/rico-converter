      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
      <rdfs:label xml:lang="fr">Paulmier, Pierre (1733-1787)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/010856_d955e84"/>
