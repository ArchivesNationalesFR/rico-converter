      <rdfs:label xml:lang="fr">Tronson, Germain</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/010487_d586e47"/>
