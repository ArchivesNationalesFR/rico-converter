      <rdfs:label xml:lang="fr">Harty, Olivier</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050098_d3726e34"/>
