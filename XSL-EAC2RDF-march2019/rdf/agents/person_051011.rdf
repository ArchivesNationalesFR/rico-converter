      <rdfs:label xml:lang="fr">Nogérée, Gaston de (1744-1809)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051011_d4395e36"/>
