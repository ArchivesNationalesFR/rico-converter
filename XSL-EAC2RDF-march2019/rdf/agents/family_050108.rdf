      <rdfs:label xml:lang="fr">Joly de Fleury (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050108_d3735e34"/>
