      <rdfs:label xml:lang="fr">Barrère, Jean (1923-....)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050899_d4317e41"/>
