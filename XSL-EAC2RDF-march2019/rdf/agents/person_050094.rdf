      <rdfs:label xml:lang="fr">Devoize, Antoine</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050094_d3722e43"/>
