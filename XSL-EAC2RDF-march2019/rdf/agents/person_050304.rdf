      <rdfs:label xml:lang="fr">Delescluze, Henri (1819-1879)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050304_d3888e72"/>
