      <rdfs:label xml:lang="fr">Painlevé, Paul (1863-1933)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050752_d4221e43"/>
