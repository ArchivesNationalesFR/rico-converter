      <rdfs:label xml:lang="fr">Boutet, Jean-Joseph</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/013023_d3122e47"/>
