      <rdfs:label xml:lang="fr">Wieviorka, Annette (1948-....)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051246_d4535e53"/>
