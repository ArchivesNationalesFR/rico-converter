      <rdfs:label xml:lang="fr">Eymard, Claude François</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050528_d4058e39"/>
