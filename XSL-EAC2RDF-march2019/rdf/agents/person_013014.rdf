      <rdfs:label xml:lang="fr">Boursier, Étienne Simon I</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/013014_d3113e47"/>
