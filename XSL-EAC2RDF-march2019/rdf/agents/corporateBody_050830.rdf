      <rdfs:label xml:lang="fr">Association des anciens élèves de l'Ecole des Roches et du Collège de Normandie (France)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050830_d4266e56"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050830_d4266e59"/>
