      <rdfs:label xml:lang="fr">Bassompierre (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050027_d3662e34"/>
