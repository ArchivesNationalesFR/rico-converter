   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/corporateBody/052021">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">France. Ministère de la Culture et de la communication. Cabinet de Fleur Pellerin, ministre (2014-2016)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/052021_d4978e95"/>
