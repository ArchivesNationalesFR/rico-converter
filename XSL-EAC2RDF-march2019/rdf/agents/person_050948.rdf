      <rdfs:label xml:lang="fr">Zylberman, Jacqueline (1928-2013)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050948_d4352e41"/>
