      <rdfs:label xml:lang="fr">Caillaux, Joseph</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050387_d3952e34"/>
