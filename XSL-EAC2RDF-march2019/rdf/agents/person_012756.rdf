      <rdfs:label xml:lang="fr">Ballin, Paul</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012756_d2855e55"/>
