      <rdfs:label xml:lang="fr">France. Ministère de l'Intérieur. Jury d'honneur (1945-1957)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051430_d4679e64"/>
