      <rdfs:label xml:lang="fr">Lescours (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050413_d3975e58"/>
