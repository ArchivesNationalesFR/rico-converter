      <rdfs:label xml:lang="fr">Vidal-Naquet, Lucien (1899-1944)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050421_d3983e36"/>
