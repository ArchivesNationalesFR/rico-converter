      <rdfs:label xml:lang="fr">Montholon (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050516_d4047e43"/>
