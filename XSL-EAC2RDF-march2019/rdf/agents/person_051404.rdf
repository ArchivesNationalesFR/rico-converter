      <rdfs:label xml:lang="fr">Gueydan, Geneviève (1963-….)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051404_d4657e45"/>
