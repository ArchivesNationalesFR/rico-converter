      <rdfs:label xml:lang="fr">Berton (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050316_d3898e41"/>
