      <rdfs:label xml:lang="fr">Dajot, Louis Lazare</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050529_d4059e41"/>
