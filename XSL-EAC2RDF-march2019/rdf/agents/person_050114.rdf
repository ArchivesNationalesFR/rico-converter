      <rdfs:label xml:lang="fr">Arago, Étienne</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050114_d3741e43"/>
