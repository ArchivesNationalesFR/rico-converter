   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/corporateBody/000376">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">France. Délégation pour l'accueil et le reclassement des rapatriés (1967-1991)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/000376_d17e50"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/000376_d17e53"/>
