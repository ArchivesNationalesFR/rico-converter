   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/corporateBody/051166">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">Forum des droits sur l'internet (Paris)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051166_d4467e45"/>
