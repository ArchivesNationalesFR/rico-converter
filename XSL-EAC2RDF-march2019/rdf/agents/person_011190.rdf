      <rdfs:label xml:lang="fr">Moine de la versine, Pierre-Charles</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011190_d1289e41"/>
