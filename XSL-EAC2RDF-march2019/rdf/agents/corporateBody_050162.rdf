      <rdfs:label xml:lang="fr">Académie Julian</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050162_d3786e34"/>
