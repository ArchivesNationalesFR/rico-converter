      <rdfs:label xml:lang="fr">Hubert, Jules</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050105_d3733e34"/>
