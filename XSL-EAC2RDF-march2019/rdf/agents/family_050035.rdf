      <rdfs:label xml:lang="fr">Gourgues (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050035_d3670e34"/>
