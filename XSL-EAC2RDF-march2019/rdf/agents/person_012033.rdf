      <rdfs:label xml:lang="fr">Faugé, Antoine-Omer</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012033_d2132e41"/>
