      <rdfs:label xml:lang="fr">Musée national de la marine (Paris ; 1827-....)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050975_d4371e54"/>
