      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Boudet (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050291_d3878e58"/>
