      <rdfs:label xml:lang="fr">Auvray, Louis</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/013169_d3268e59"/>
