      <rdfs:label xml:lang="fr">Maison d'enfants du Quercy (France)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050852_d4280e41"/>
