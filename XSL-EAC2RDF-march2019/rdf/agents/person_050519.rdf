      <rdfs:label xml:lang="fr">Treilhard, Jean-Baptiste (1742-1810)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050519_d4049e45"/>
