      <rdfs:label xml:lang="fr">Lally-Tollendal (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050919_d4335e34"/>
