      <rdfs:label xml:lang="fr">Vingtain, Jean</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/010390_d489e41"/>
