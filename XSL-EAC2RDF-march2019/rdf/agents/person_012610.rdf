      <rdfs:label xml:lang="fr">Chaudot, Vivant-Jean-Baptiste</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012610_d2709e45"/>
