      <rdfs:label xml:lang="fr">Fortoul (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050582_d4111e69"/>
