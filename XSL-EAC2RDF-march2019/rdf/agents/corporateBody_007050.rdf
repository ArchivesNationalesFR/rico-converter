      <rdfs:label xml:lang="fr">Inspection générale de l'équipement</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/007050_d161e44"/>
