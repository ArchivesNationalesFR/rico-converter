   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/family/050524">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Du Bouchet de Sourches (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050524_d4054e41"/>
