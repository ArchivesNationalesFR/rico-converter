   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/family/050608">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Eprémesnil (famille Duval d')</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050608_d4125e39"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050608_d4125e42"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050608_d4125e45"/>
