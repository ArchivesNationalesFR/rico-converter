      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Liberge de Granchain (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051448_d4695e67"/>
