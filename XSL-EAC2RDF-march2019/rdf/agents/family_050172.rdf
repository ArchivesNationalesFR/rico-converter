      <rdfs:label xml:lang="fr">Deschanel (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050172_d3795e69"/>
