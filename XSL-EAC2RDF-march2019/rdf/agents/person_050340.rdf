      <rdfs:label xml:lang="fr">Brusset, Max (1909-1992)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050340_d3918e45"/>
