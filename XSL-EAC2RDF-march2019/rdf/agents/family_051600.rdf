      <rdfs:label xml:lang="fr">Boullongne de Préninville (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/051600_d4813e52"/>
