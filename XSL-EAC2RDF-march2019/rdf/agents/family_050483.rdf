      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Rostaing (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050483_d4024e41"/>
