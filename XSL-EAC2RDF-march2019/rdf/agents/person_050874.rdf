   <rdf:Description rdf:about="http://data.archives-nationales.culture.gouv.fr/person/050874">
      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
      <rdfs:label xml:lang="fr">Pène, Pierre (1898-1972)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050874_d4300e70"/>
