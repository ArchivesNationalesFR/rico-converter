      <rdfs:label xml:lang="fr">Malesherbes, Chrétien-Guillaume de Lamoignon de</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050081_d3711e43"/>
