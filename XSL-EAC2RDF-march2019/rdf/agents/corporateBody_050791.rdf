      <rdfs:label xml:lang="fr">Fondation de Wendel (France)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050791_d4246e36"/>
