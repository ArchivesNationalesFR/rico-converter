      <rdfs:label xml:lang="fr">Rougé (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050311_d3895e79"/>
