      <rdfs:label xml:lang="fr">Dupont, Damien-Louis</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012193_d2292e55"/>
