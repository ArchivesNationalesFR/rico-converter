      <rdfs:label xml:lang="fr">Bro, Jean-Louis</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/013061_d3160e41"/>
