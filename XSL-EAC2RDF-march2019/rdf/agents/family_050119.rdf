      <rdfs:label xml:lang="fr">Lobel-Mahy (famille de)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050119_d3746e43"/>
