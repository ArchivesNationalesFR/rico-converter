      <rdfs:label xml:lang="fr">Donnet, François Ferdinand Auguste (cardinal)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050612_d4128e43"/>
