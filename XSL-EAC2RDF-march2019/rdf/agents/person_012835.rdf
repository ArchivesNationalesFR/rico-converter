      <rdfs:label xml:lang="fr">Bellanger, Jérôme</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012835_d2934e47"/>
