      <rdfs:label xml:lang="fr">Deherain, Pierre (1743-1806)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012362_d2461e71"/>
