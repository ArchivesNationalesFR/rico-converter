      <rdfs:label xml:lang="fr">Moreau, Jean-Bélizaire</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050337_d3915e39"/>
