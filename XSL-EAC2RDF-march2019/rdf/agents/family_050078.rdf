      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#Family"/>
      <rdfs:label xml:lang="fr">Phélypeaux (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050078_d3708e50"/>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050078_d3708e53"/>
