      <rdfs:label xml:lang="fr">Paliard, Nicolas Augustin (baron)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050525_d4055e39"/>
