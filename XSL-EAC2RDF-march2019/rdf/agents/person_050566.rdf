      <rdfs:label xml:lang="fr">Duguet, Jacques Joseph (1649-1733)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050566_d4096e43"/>
