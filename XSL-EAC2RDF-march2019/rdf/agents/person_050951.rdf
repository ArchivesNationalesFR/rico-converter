      <rdfs:label xml:lang="fr">Gerhardt, Gustave-Adolphe (1843-1921)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050951_d4355e41"/>
      <RiC:birthDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1843-02-02</RiC:birthDate>
