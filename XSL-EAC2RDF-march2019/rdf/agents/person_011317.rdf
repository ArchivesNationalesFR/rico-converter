      <rdfs:label xml:lang="fr">Laisné, Louis-Claude</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011317_d1416e41"/>
