      <rdfs:label xml:lang="fr">La Pomélie (famille)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050363_d3935e40"/>
