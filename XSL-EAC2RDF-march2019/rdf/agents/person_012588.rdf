      <rdfs:label xml:lang="fr">Chapellier, Charles-Etienne</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/012588_d2687e41"/>
