      <rdfs:label xml:lang="fr">Blanchot, Raymond</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050140_d3765e43"/>
