      <rdf:type rdf:resource="http://www.ica.org/standards/RiC/ontology#CorporateBody"/>
      <rdfs:label xml:lang="fr">Pôle de conservation des archives des associations de jeunesse et d'éducation populaire (France)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050739_d4213e58"/>
