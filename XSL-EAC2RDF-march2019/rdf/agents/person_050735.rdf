      <rdfs:label xml:lang="fr">Fraisse, Paul (1911-1996)</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050735_d4210e43"/>
