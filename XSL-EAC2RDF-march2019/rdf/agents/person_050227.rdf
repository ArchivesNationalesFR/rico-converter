      <rdfs:label xml:lang="fr">Viansson-Ponté, Pierre</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/050227_d3833e34"/>
