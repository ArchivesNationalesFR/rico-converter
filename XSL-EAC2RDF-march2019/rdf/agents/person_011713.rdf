      <rdfs:label xml:lang="fr">Judde, Jacques</rdfs:label>
      <RiC:thingIsSourceOfRelationToAppellation rdf:resource="http://data.archives-nationales.culture.gouv.fr/relationToAppellation/011713_d1812e41"/>
