            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A relation between a person and a position that the person
            occupies Shortcut : occupies object property</rdfs:comment>
        <rdfs:label xml:lang="en">Position Holding Relation</rdfs:label>
    </owl:Class>
    


