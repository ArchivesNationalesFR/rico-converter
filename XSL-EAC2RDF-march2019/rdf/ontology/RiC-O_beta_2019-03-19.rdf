    <!-- http://www.ica.org/standards/RiC/ontology#AcquaintanceRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AcquaintanceRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#acquaintanceRelationConnects"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A relation between two persons who know (have met, have
            corresponded with...) each other. Shortcut : hasAcquaintance</rdfs:comment>
        <rdfs:label xml:lang="en">Acquaintance Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#Action -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#Action">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#hasPurpose"/>
                <owl:allValuesFrom rdf:resource="http://www.ica.org/standards/RiC/ontology#Purpose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#isOriginationOf"/>
                <owl:allValuesFrom rdf:resource="http://www.ica.org/standards/RiC/ontology#RecordResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#isPartOf"/>
                <owl:allValuesFrom rdf:resource="http://www.ica.org/standards/RiC/ontology#Process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#performedBy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#history"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A discrete act performed by an Agent.</rdfs:comment>
        <rdfs:label xml:lang="en">Action</rdfs:label>
        <skos:definition xml:lang="en">A discrete act performed by an Agent.</skos:definition>
        <skos:historyNote xml:lang="en">AUgust 23 2018: Activity renamed Action, and new Activity
            class created as a subclass of Thing for now</skos:historyNote>
        <skos:note xml:lang="en">QUESTION: Action is now considered a subclass of Event (it is an
            event that involves actively at least one agent). Do we agree?</skos:note>
        <skos:prefLabel xml:lang="en">Action</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Action is a part of Process</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#Activity -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#Activity">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#consistsOf"/>
