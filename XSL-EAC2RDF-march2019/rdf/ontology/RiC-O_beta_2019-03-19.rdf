        <skos:definition xml:lang="en">A guide, plan, or design for Activity.</skos:definition>
        <skos:historyNote xml:lang="en">Created August 23 2018</skos:historyNote>
        <skos:prefLabel xml:lang="en">Rule</skos:prefLabel>
